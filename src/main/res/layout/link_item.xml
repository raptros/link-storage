<?xml version="1.0" encoding="utf-8"?>
<local.nodens.linkstorage.LinkItem xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView android:text="none"
        android:textSize="16dp"
        android:paddingLeft="16dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:gravity="center_vertical|left"
        android:id="@+id/link_title_view" />
    <TextView android:text="none"
        android:textSize="16dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:paddingRight="16dp"
        android:gravity="center_vertical|right"
        android:id="@+id/link_url_view" />
</local.nodens.linkstorage.LinkItem>
